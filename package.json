{
  "name": "el-admin-layout",
  "version": "0.8.5",
  "description": "基于element-ui的后台管理layout",
  "main": "src/index.js",
  "module": "dist/esm/index.js",
  "jsdelivr": "dist/index.umd.min.js",
  "types": "types/index.d.ts",
  "author": "toesbieya",
  "license": "MIT",
  "homepage": "https://github.com/toesbieya/el-admin-layout/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toesbieya/el-admin-layout.git"
  },
  "bugs": {
    "url": "https://github.com/toesbieya/el-admin-layout/issues"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:example": "vue-cli-service build",
    "build:lib-umd": "vue-cli-service build --target lib --no-clean --formats umd-min --name index build/umd.js",
    "build:lib-esm": "node build/esm.js",
    "build:css": "node build/css.js",
    "dist": "npm run build:lib-umd && npm run build:lib-esm && npm run build:css"
  },
  "peerDependencies": {
    "element-ui": "^2.15.0",
    "vue": "^2.6.12",
    "vue-router": "^3.3.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.0-alpha.5",
    "@vue/cli-service": "^5.0.0-alpha.5",
    "element-ui": "^2.15.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "vue": "^2.6.12",
    "vue-router": "^3.5.1",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.6.2"
  },
  "browserslist": [
    "Chrome > 80"
  ]
}
